<h1>新規記事投稿</h1>
  <%= form_for @post do |f| %>
  <p>
   <%= f.label :title %>
   <%= f.text_field :title %>
  </p>
  <p>
    <%= f.label :category %>
    <%= f.text_field :category %>
  </p>
  <p>
    <%= f.label :body %>
    <%= f.text_area :body %>
  </p>

  <p>
    <%= f.submit"新規投稿" %>
  </p>

<% end %>
